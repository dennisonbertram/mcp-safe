{
  "name": "safe-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for SAFE multisig wallet management",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "hardhat:node": "npx hardhat node",
    "deploy:local": "npx hardhat run scripts/deploy-safe-contracts.js --network localhost",
    "deploy:real": "npx hardhat run scripts/deploy-real-safe-sdk.js --network localhost",
    "dev:local": "USE_REAL_TOOLS=true npm start",
    "test:integration": "USE_REAL_TOOLS=true npm test -- --testPathPattern=integration"
  },
  "keywords": [
    "mcp",
    "safe",
    "multisig",
    "blockchain",
    "ethereum"
  ],
  "author": "Safe MCP Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@safe-global/api-kit": "^2.5.11",
    "@safe-global/protocol-kit": "^4.1.7",
    "@safe-global/relay-kit": "^3.0.0",
    "@safe-global/safe-contracts": "^1.4.1",
    "@safe-global/safe-core-sdk-types": "^5.1.0",
    "@safe-global/safe-deployments": "^1.37.34",
    "dotenv": "^17.2.1",
    "ethers": "^6.14.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.9",
    "@nomicfoundation/hardhat-ethers": "^3.0.9",
    "@nomicfoundation/hardhat-ignition": "^0.15.11",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.12",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.14",
    "@nomicfoundation/ignition-core": "^0.15.11",
    "@safe-global/safe-singleton-factory": "^1.0.44",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.24.2",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "solidity-coverage": "^0.8.16",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typechain": "^8.3.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
