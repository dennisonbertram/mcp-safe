{
  "name": "safe-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for SAFE multisig wallet management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "hardhat:node": "npx hardhat node",
    "deploy:local": "npx hardhat run scripts/deploy-safe-contracts.js --network localhost",
    "dev:local": "USE_REAL_TOOLS=true npm start",
    "test:integration": "USE_REAL_TOOLS=true npm test -- --testPathPattern=integration"
  },
  "keywords": [
    "mcp",
    "safe",
    "multisig",
    "blockchain",
    "ethereum"
  ],
  "author": "Safe MCP Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@safe-global/api-kit": "^2.5.11",
    "@safe-global/protocol-kit": "^4.1.7",
    "@safe-global/relay-kit": "^3.0.0",
    "@safe-global/safe-core-sdk-types": "^5.1.0",
    "ethers": "^6.14.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.24.2",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
